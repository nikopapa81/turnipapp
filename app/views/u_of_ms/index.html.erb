<div class="page-header">
  <h1>U of ms</h1>
</div>

<p><a href="/u_of_ms/new" class="btn btn-primary">Add a New U Of M</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Unit name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @u_of_ms.each do |u_of_m| %>
      <tr>
        <td><%= u_of_m.unit_name %></td>
        <td>
          <a href="/u_of_ms/<%= u_of_m.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/u_of_ms/<%= u_of_m.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_u_of_m/<%= u_of_m.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

